{
    "name": "datalogix/laravel-utils",
    "description": "A Laravel package that provides a set of useful utilities and service providers to enhance your application.",
    "keywords": ["laravel", "utils", "laravel-utils"],
    "homepage": "https://github.com/datalogix/laravel-utils",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Gobbo de Souza",
            "email": "ricardogobbosouza@yahoo.com.br"
        }
    ],
    "require": {
        "php": "^8.2",
        "datalogix/laravel-builder-macros": "^4.0",
        "datalogix/laravel-sensible": "^1.0",
        "datalogix/laravel-validation": "^3.0",
        "illuminate/support": "^11.39|^12.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^6.2",
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Datalogix\\Utils\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Datalogix\\Utils\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datalogix\\Utils\\UtilsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
