{
    "name": "datalogix/laravel-utils",
    "description": "Laravel Utils is a package that configure simple things, which we usually forget about in our projects.",
    "keywords": [
        "laravel",
        "utils",
        "translations",
        "model",
        "locale",
        "schema",
        "https",
        "paginator"
    ],
    "homepage": "https://github.com/datalogix/laravel-utils",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Gobbo de Souza",
            "email": "ricardogobbosouza@yahoo.com.br"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "datalogix/laravel-translation": "^1.1|^2.0",
        "datalogix/laravel-validation": "^1.3|^2.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/pagination": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^6.2",
        "phpunit/phpunit": "^9.5|^10.5"
    },
    "autoload": {
        "psr-4": {
            "Datalogix\\Utils\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Datalogix\\Utils\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datalogix\\Utils\\UtilsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
