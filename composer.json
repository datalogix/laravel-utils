{
    "name": "datalogix/laravel-utils",
    "description": "Laravel Utils is a package that configure simple things, which we usually forget about in our projects.",
    "keywords": [
        "laravel",
        "utils",
        "locale",
        "schema",
        "https",
        "paginator"
    ],
    "homepage": "https://github.com/datalogix/laravel-utils",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Gobbo de Souza",
            "email": "ricardogobbosouza@yahoo.com.br"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/contracts": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/pagination": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "spatie/phpunit-watcher": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Datalogix\\Utils\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Datalogix\\Utils\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datalogix\\Utils\\UtilsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "watch": "vendor/bin/phpunit-watcher watch"
    }
}
